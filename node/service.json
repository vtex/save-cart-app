{
  "stack": "nodejs",
  "memory": 1024,
  "ttl": 300,
  "timeout": 60,
  "routes": {
    "getSetupConfig": {
      "path": "/_v/0/savecart/config",
      "public": false
    },
    "getRepresentative": {
      "path": "/_v/0/savecart/representative",
      "public": false
    },
    "getCarts": {
      "path": "/_v/0/savecart/:email/carts/",
      "public": false
    },
    "saveCart": {
      "path": "/_v/0/savecart/carts/",
      "public": false
    },
    "removeCart": {
      "path": "/_v/0/savecart/carts/:cartId",
      "public": false
    },
    "useCart": {
      "path": "/_v/0/savecart/carts/:orderFormId/use",
      "public": false
    }
  }
}
